{
    "manifest_version": 2,
    "name": "Super Speedy Dial",
    "version": "1.1.1",

    "browser_specific_settings": {
        "gecko": {
            "id": "wilshomepageaddon@gmail.com"
        }
    },
  
    "description": "My home page",

    "browser_action": {
        "default_icon": "logo/logo_small.png",
        "default_title": "Super Speedy Dial"
    },

    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "chrome_settings_overrides": {
        "homepage": "index.html"
    },

    "permissions": [
        "activeTab",
        "bookmarks",
        "storage",
        "tabs",
        "tabHide",
        "unlimitedStorage",
        "<all_urls>"
    ],

    "web_accessible_resources": [
        "images/default_background.jpg",
        "lib/mithril.js",
        "src/main.js"
    ]
}